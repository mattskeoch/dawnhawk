        "781c40cf-5d88-45fa-9a2f-eaabf360892b": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/From-Manufacturing-To-Installation-Icon.png",
            "title": "From Manufacturing to Installation",
            "text": "<p>We are the only West Australian installation company with its own local <br\/>manufacturing plant. We can handle the entire process from production to<br\/> installation to save you money.<\/p>",
            "link_label": "",
            "link": ""
