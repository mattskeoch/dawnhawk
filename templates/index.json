    "60ceb3dc-eb47-44cd-aa8b-994de85622f8": {
      "type": "image-with-text",
      "blocks": {
        "template--20175700066578__60ceb3dc-eb47-44cd-aa8b-994de85622f8-heading-1": {
          "type": "heading",
